<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.ecnu.mapper.CorpusMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.ecnu.model.CorpusDO">
        <id column="id" property="id" />
        <result column="pinyin" property="pinyin" />
        <result column="ref_text" property="refText" />
        <result column="eval_mode" property="evalMode" />
        <result column="difficulty" property="difficulty" />
        <result column="creator" property="creator" />
        <result column="del" property="del" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, pinyin, ref_text, eval_mode, difficulty, creator, del, gmt_create, gmt_modified
    </sql>

</mapper>
